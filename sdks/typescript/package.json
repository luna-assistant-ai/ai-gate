{
  "name": "@ai-gate/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for AI Gate - OpenAI Realtime API Proxy",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai-gate",
    "openai",
    "realtime-api",
    "webrtc",
    "turn",
    "sdk",
    "typescript"
  ],
  "author": "AI Gate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luna-assistant-ai/ai-gate.git",
    "directory": "sdks/typescript"
  },
  "homepage": "https://www.ai-gate.dev",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
